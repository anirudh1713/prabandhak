{
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build",
        "grpahql:codegen"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {},
    "test": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "coverage/**"
      ]
    },
    "dev": {
      "cache": false,
      "dependsOn": [
        "graphql:codegen"
      ]
    },
    "graphql:codegen": {
      "outputs": [
        "apps/api/src/generated/**"
      ]
    }
  }
}
